    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <header>
    <h1>AUTO GRADING SYSTEM</h1>
    <nav>
        <ul>
            <li><a href="/">Home</a></li>
            <li><a href="#">Q&A</a></li>
            <li>
              <% if(userInformation){ %>
                <li><a href="/login">Login</a></li>
             <!-- <input type="button" value="login" onClick="self.location='/login';"> -->
              <% } else{ %>
              <!-- <input type="button" value="logout" onClick="self.location='/logout';"> -->
                <li><a href="/logout">Logout</a></li>
              <% } %>
            </li>
        </ul>
  </nav>
  </header>
<article>
    <h1>Assignment List</h1>
    <h3>Total : <%=list.length %></h3>


    <table class = "type11"  width = "100%">
      <tr>
        <th>과제</th>
        <th>제목</th>
        <th>제출 기간</th>
      </tr>
      <% for(var i=0; i<list.length; i++) {%>
        <tr>
          <td>
            #<%=i%>
          </td>
          <td>
            <a href=<%= list[i]['title'] %>><%= list[i]['title'] %></a>
          </td>
          <td>
            <% if(list[i]['end_date']) {%>
            <%= list[i]['start_date'].toLocaleString()%>
            ~
            <%= list[i]['end_date'].toLocaleString()%>
            <%}%>
          </td>
        </tr>
        <% } %>
    </table>
    <form action="./" method="post">
      <% if(user_idx=='1234') {%>
      <input type="submit" value="과제 등록">
      <%}%>
    </form>
</article>
